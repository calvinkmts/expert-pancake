{
  "$schema": "https://www.krakend.io/schema/v3.json",
  "version": 3,
  "name": "KrakenD - API Gateway",
  "timeout": "3000ms",
  "cache_ttl": "300s",
  "output_encoding": "json",
  "disable_rest": true,
  "extra_config": {
    "security/cors": {
      "allow_origins": [
        "*"
      ],
      "allow_methods": [
        "GET",
        "HEAD",
        "POST"
      ],
      "expose_headers": [
        "Content-Length",
        "Content-Type"
      ],
      "allow_headers": [
        "Accept-Language"
      ],
      "max_age": "12h",
      "allow_credentials": false,
      "debug": false
    }
  },
  "endpoints": [
    {
      "endpoint": "/location/provinces",
      "method": "GET",
      "output_encoding": "json",
      "backend": [
        {
          "url_pattern": "/api-wilayah-indonesia/api/provinces.json",
          "encoding": "json",
          "sd": "static",
          "method": "GET",
          "host": [
            "https://calvinkmts.github.io"
          ],
          "disable_host_sanitize": false,
          "is_collection": true,
          "mapping": {
            "collection": "result"
          }
        }
      ]
    },
    {
      "endpoint": "/location/regencies/{provinceId}",
      "method": "GET",
      "output_encoding": "json",
      "backend": [
        {
          "url_pattern": "/api-wilayah-indonesia/api/regencies/{provinceId}.json",
          "encoding": "json",
          "sd": "static",
          "method": "GET",
          "host": [
            "https://calvinkmts.github.io"
          ],
          "disable_host_sanitize": false,
          "is_collection": true,
          "mapping": {
            "collection": "result"
          }
        }
      ]
    },
    {
      "endpoint": "/location/districts/{regencyId}",
      "method": "GET",
      "output_encoding": "json",
      "backend": [
        {
          "url_pattern": "/api-wilayah-indonesia/api/districts/{regencyId}.json",
          "encoding": "json",
          "sd": "static",
          "method": "GET",
          "host": [
            "https://calvinkmts.github.io"
          ],
          "disable_host_sanitize": false,
          "is_collection": true,
          "mapping": {
            "collection": "result"
          }
        }
      ]
    },
    {
      "endpoint": "/location/villages/{districtId}",
      "method": "GET",
      "output_encoding": "json",
      "backend": [
        {
          "url_pattern": "/api-wilayah-indonesia/api/villages/{districtId}.json",
          "encoding": "json",
          "sd": "static",
          "method": "GET",
          "host": [
            "https://calvinkmts.github.io"
          ],
          "disable_host_sanitize": false,
          "is_collection": true,
          "mapping": {
            "collection": "result"
          }
        }
      ]
    }
  ]
}